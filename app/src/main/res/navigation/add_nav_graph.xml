<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/center_nav_graph"
    app:startDestination="@id/bottom_nav_graph_center_start_destination">

    <fragment
        android:id="@id/bottom_nav_graph_center_start_destination"
        android:name="com.velord.composescreenexample.ui.main.bottomNav.addGraph.AddGraphFragment"
        android:label="AddGraphFragment" >

        <action
            android:id="@+id/toInDevelopmentFragment"
            app:destination="@id/inDevelopmentFragment"/>

        <action
            android:id="@+id/from_addGraphFragment_to_shapeDemoFragment"
            app:destination="@id/shapeDemoFragment"
            app:enterAnim="@anim/slide_in_up"
            app:popExitAnim="@anim/slide_in_down"
            />
    </fragment>

    <fragment
        android:id="@+id/inDevelopmentFragment"
        android:name="com.velord.composescreenexample.ui.main.inDevelopment.InDevelopmentFragment"
        android:label="InDevelopmentFragment"  />

    <fragment
        android:id="@+id/shapeDemoFragment"
        android:name="com.velord.shapedemo.ShapeDemoFragment"
        android:label="ShapeDemoFragment" />

</navigation>